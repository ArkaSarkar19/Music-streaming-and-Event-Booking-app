<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Ui.SignupLoginMain.LoginBox">
   <children>
      <ImageView fitHeight="728.0" fitWidth="1292.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../Resources/homepage.png" />
         </image>
      </ImageView>
      <TextField id="loginusername" fx:id="mainLoginUsername" layoutX="513.0" layoutY="376.0" opacity="0.25" prefHeight="38.0" prefWidth="267.0" promptText="Email">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <Button id="loginbutton" fx:id="mainlogin" layoutX="518.0" layoutY="480.0" mnemonicParsing="false" opacity="0.49" prefHeight="39.0" prefWidth="259.0" styleClass="loginbutton" stylesheets="@Main.css" text="LOGIN">
         <font>
            <Font name="System Bold" size="16.0" />
         </font></Button>
      <Hyperlink layoutX="367.0" layoutY="262.0" />
      <Hyperlink fx:id="mainLoginSignup" layoutX="617.0" layoutY="545.0" text="Sign Up">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Hyperlink>
      <PasswordField id="loginpassword" fx:id="mainLoginPassword" layoutX="513.0" layoutY="426.0" opacity="0.25" prefHeight="38.0" prefWidth="267.0" promptText="*************" />
   </children>
</AnchorPane>
