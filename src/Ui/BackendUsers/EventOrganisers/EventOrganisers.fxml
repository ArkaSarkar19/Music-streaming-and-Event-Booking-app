<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Ui.BackendUsers.EventOrganisers.EventOrganisersController">
    <children>
        <ImageView fitHeight="720.0" fitWidth="1280.0" layoutX="-10.0" layoutY="-13.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../Resources/bg.jpg" />
            </image>
            <effect>
                <GaussianBlur />
            </effect>
        </ImageView>
        <VBox alignment="TOP_CENTER" layoutX="55.0" layoutY="166.0" prefHeight="420.0" prefWidth="216.0" spacing="15.0">
            <children>
                <Button mnemonicParsing="false" onAction="#handleAddLivePerformance" stylesheets="@EventOrganisers.css" text="Add Live Performance" />
                <Button mnemonicParsing="false" onAction="#handleCheckLikes" stylesheets="@EventOrganisers.css" text="Check Likes/views" />
                <Button mnemonicParsing="false" onAction="#handleCheckBooking" stylesheets="@EventOrganisers.css" text="Check Bookings" />
                <Button mnemonicParsing="false" onAction="#handleCheckSchedule" stylesheets="@EventOrganisers.css" text="Check Schedule" />
                <Button mnemonicParsing="false" onAction="#handleAllTickets" stylesheets="@EventOrganisers.css" text="All Tickets" />
