<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Ui.BackendUsers.MusicProffesionals.MusicProfController">
   <children>
      <ImageView fitHeight="720.0" fitWidth="1280.0" layoutX="-10.0" layoutY="-13.0" pickOnBounds="true" preserveRatio="true">
