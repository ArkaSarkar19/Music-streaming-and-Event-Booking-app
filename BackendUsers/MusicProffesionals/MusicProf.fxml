         <effect>
            <GaussianBlur />
         </effect>
      </ImageView>
      <VBox alignment="TOP_CENTER" layoutX="55.0" layoutY="166.0" prefHeight="372.0" prefWidth="197.0" spacing="15.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handleAddsong" stylesheets="@MusicProf.css" text="Add Song" />
            <Button mnemonicParsing="false" onAction="#handleAddAlbum" stylesheets="@MusicProf.css" text="Add album" />
            <Button mnemonicParsing="false" onAction="#handleRemoveSong" stylesheets="@MusicProf.css" text="Remove song" />
